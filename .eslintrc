{
  parser: "babel-eslint",
  parserOptions: {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "classes": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "quotes": [1, "single", {"allowTemplateLiterals": true}],
    "semi": [1, "always"],
    "react/prefer-es6-class": [1, "always"],
    "react/no-find-dom-node": 0,
    "no-console": 0
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": [
    "react"
  ]
}
