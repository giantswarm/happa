{
  "name": "happa",
  "private": true,
  "src": "src",
  "test": "test",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js,.ts,.tsx ./src/",
    "lint:fix": "yarn lint --fix",
    "lint:css": "./node_modules/.bin/stylelint './src/**/*.{js,ts,tsx}'",
    "start": "./node_modules/.bin/webpack-dev-server --open --progress --colors --config ./webpack.dev.js",
    "test": "./node_modules/.bin/jest",
    "test:watch": "yarn test --watch",
    "test:clean-cache": "yarn test --clearCache",
    "test:coverage": "yarn test --collect-coverage",
    "format": "./node_modules/.bin/prettier \"src/**/*.{js,ts,tsx}\"",
    "prettier": "yarn format --write",
    "validate-prettier": "yarn format --version; yarn format --no-color --check",
    "precommit": "./node_modules/.bin/lint-staged",
    "typecheck": "./node_modules/.bin/tsc",
    "dist": "./node_modules/.bin/webpack --config webpack.prod.js"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    "helm",
    ".vscode",
    ".github",
    ".circleci",
    "scripts"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      [
        "@babel/preset-typescript",
        {
          "allExtensions": true,
          "isTSX": true
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "emotion",
      "react-hot-loader/babel"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ],
          "@babel/preset-react"
        ]
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
  },
  "dist": "dist",
  "mainInput": "main",
  "mainOutput": "main",
